const setDefaultConsentState = require('setDefaultConsentState');

function grantedOrDenied(consentString, name){
  var k = name + ":1";
  return consentString.indexOf(k)!=-1 ? "granted":"denied";
}

setDefaultConsentState({
  ad_storage: data.ad_storage ? "granted":"denied",
  analytics_storage: data.analytics_storage ? "granted":"denied",
  ad_user_data: data.ad_user_data ? "granted":"denied",
  ad_personalization: data.ad_personalization ? "granted":"denied",
  functionality_storage: data.functionality_storage ? "granted":"denied",
  personalization_storage: data.personalization_storage ? "granted":"denied",
  security_storage: data.security_storage ? "granted":"denied",
  wait_for_update: 1000
});

// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


